{
  "manifest_version": 3,
  "name": "Dynamics CRM Entity Getter",
  "version": "1.0",
  "permissions": ["scripting", "tabs"],
  "host_permissions": ["*://*.dynamics.com/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.dynamics.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "/worker.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
